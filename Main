local next = script.Parent.Name+1

if script.Parent.Values.QuestionType == "MultipleChoice" then
     
     script.Parent.Tests.MultipleChoice.Active = true
     
     script.Parent.Tests.MultipleChoice.Question.text = script.Parent.QuestionValues.Question
     
     
     script.Parent.Tests.MultipleChoice.Choice1.MouseButton1Click:Connect() function
          
          --VERSION 3.1 CHANGE START
          
          script.Parent.Tests.Confirm.Visible = true 
          
          script.Parent.Tests.Confirm.Yes.MouseButton1Click:Connect() function()
          
               if script.Parent.Answer == A then
                    script.Parent.Parent.Values.CorrectCount = script.Parent.Parent.Values.CorrectCount + 1
                    script.Parent.Enabled = false 
                    
                    game.Players:WaitForChild(game.Workspace:WaitForChild(Model).Name).PlayerGUI.next.Enabled = true
                    
                    
               else
                    script.Parent.Parent.Values.IncorrectCount = script.Parent.Parent.Values.IncorrectCount + 1
                    
                    script.Parent.Enabled = false 
                    
                    game.Players:WaitForChild(game.Workspace:WaitForChild(Model).Name).PlayerGUI.next.Enabled = true
               end 
          end
          
          script.Parent.Tests.Confirm.No.MouseButton1Click:Connect() function()
                script.Parent.Tests.Confirm.Visible = false
          end
          
          --VERSION 3.1 CHANGE END
     end
     
     script.Parent.Tests.MultipleChoice.Choice2.MouseButton1Click:Connect() function
